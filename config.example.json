{
  "blog_url": "example.tumblr.com",
  "output_dir": "./downloads",
  "tumblr_api_key": "your_api_key_here",
  
  "resume": true,
  "include_reblogs": true,
  "download_embeds": false,
  "recover_removed_media": true,
  
  "wayback_enabled": true,
  "wayback_max_snapshots": 5,
  
  "rate_limit": 1.0,
  "concurrency": 2,
  "max_retries": 3,
  "base_backoff": 1.0,
  "max_backoff": 32.0,
  
  "verbose": false,
  "dry_run": false
}
